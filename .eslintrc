{
	"parser": "@babel/eslint-parser",
	"extends": [
		"airbnb"
	],
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"indent": [
			"error",
			4
		],
		"semi": 0,
		"react/jsx-indent": [
			"error",
			4
		],
		"react/jsx-indent-props": [
			"error",
			4
		],
		"react/prop-types": 0, // disable for now
		"no-unused-vars": "warn"
	}
}